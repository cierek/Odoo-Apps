<?xml version="1.0" encoding="utf-8"?>
<data>
  <!-- this removes Google+ from blog list -->
  <template id="blog_googleplus_remove0" inherit_id="website_blog.blog_post_short" name="Remove Google+ Blog List" active="True" priority="99">
    <xpath expr='//a[@class="fa fa-google-plus-square o_google"]' position="replace"></xpath>
  </template>

</data>
